{
  "version": 2,
  "projects": [
    {
      "name": "frontend",
      "rootDirectory": "FrontEnd/vite-project",
      "builds": [
        {
          "src": "package.json",
          "use": "@vercel/static-build",
          "config": { "distDir": "dist" }
        }
      ],
      "routes": [
        { "src": "/(.*)", "dest": "/$1" }
      ]
    }
  ]
}
